@page "{id:int}"
@using ReAl.Template.SbAdmin2.Pages
@model ReAl.Template.SbAdmin2.Pages.SegAplicaciones.EditModel

@section Styles
{

}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Edicion - @Model.MiAplicacion.aplicacionsap</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Basic Form Elements
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-lg-6">
                        <form role="form" method="post">
                            <div class="form-group">
                                <label asp-for="MiAplicacion.aplicacionsap"></label>
                                <input asp-for="MiAplicacion.aplicacionsap" class="form-control"></input>
                                <span asp-validation-for="MiAplicacion.aplicacionsap" ></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="MiAplicacion.descripcionsap"></label>
                                <input asp-for="MiAplicacion.descripcionsap" class="form-control"></input>
                                <span asp-validation-for="MiAplicacion.descripcionsap" ></span>
                            </div>
                            <button type="submit" class="btn btn-default">Actualizar</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts
{

}
